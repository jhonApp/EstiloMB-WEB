@{
    ViewData["Title"] = "Login";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="~/css/site.css" />

    <script type="text/javascript" src="~/js/fa/all.js"></script>
    <script type="text/javascript" src="~/js/ChartJS/Chart.js"></script>
    <script type="text/javascript" src="~/js/site.js"></script>
    <script src="~/js/swipe.js"></script>
</head>
<div class="nav-main-sacola">
    <img class="logo" src="~/image/logo-oficial.png" alt=""/>
</div>
<content>
    <div class="center center-sigup">
        <h2>Vamos criar o seu acesso?</h2>
        <form method="post">
            <div class="col-flex">
                <div class="txt_field">
                    <input type="text" name="" value="" required/>
                    <label>CPF</label>
                </div>
                <div class="txt_field">
                    <input type="text" name="" value="" required/>
                    <label>Qual é o seu nome?</label>
                </div>
                <div class="txt_field">
                    <input type="password" name="" value="" required/>
                    <label>Crie uma nova senha</label>
                </div>
                <div class="txt_field">
                    <input type="password" name="" value="" required/>
                    <label>Confirme a sua senha</label>
                </div>
                
                <div class="txt_field">
                    <input type="text" name="" value="" required/>
                    <label>Email</label>
                </div>
                <div class="txt_field">
                    <input type="text" name="" value="" required/>
                    <label>Data Nascimento</label>
                </div>
                <div class="txt_field">
                    <input type="text" name="" value="" required/>
                    <label>Seu Celular</label>
                </div>
            </div>
            <input type="submit" value="Confirmar"/>
        </form>
    </div>
</content>
